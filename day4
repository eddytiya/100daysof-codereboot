############  coin toss program
  import random
toss=random.randint(0,1)

if toss==1:
  print("Heads")
  
else:
  print("Tails")

######state of india(lists)
  stateofindia=['karnataka','kerala','tamilnadu','telangana','andhra']

print(stateofindia[0])
print(stateofindia[4])
stateofindia.append("goa")
print(stateofindia)

stateofindia.extend(["gujarat","punjab"])
print(stateofindia)
  
